{
    "name": "acerex/filament-menux",
    "description": "Good or bad, you decide.",
    "keywords": [
        "AceREx",
        "laravel",
        "filament-menux"
    ],
    "homepage": "https://github.com/acerex/filament-menux",
    "support": {
        "issues": "https://github.com/acerex/filament-menux/issues",
        "source": "https://github.com/acerex/filament-menux"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "AceREx",
            "email": "ace.rexnoir@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "kalnoy/nestedset": "^6.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "AceREx\\FilamentMenux\\": "src/",
            "AceREx\\FilamentMenux\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AceREx\\FilamentMenux\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "build": "node bin/build.js",
        "dev": "node bin/build.js --dev",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AceREx\\FilamentMenux\\FilamentMenuxServiceProvider"
            ],
            "aliases": {
                "FilamentMenux": "AceREx\\FilamentMenux\\Facades\\FilamentMenux"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
